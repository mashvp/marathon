{
  "name": "@mashvp/marathon",
  "version": "0.1.4",
  "description": "Marathon runner system for vanilla JavaScript",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "author": "Mashvp",
  "license": "MIT",
  "private": false,
  "scripts": {
    "watch": "parcel $npm_package_main -d dist/ -o marathon.js",
    "build": "parcel build $npm_package_main -d dist/ -o marathon.min.js",
    "demo": "parcel demo/index.html -d tmp/",
    "doc": "jsdoc --configure .jsdoc.json --verbose",
    "open-doc": "serve ./docs/marathon/0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "jsdoc": "^3.6.3",
    "minami": "^1.2.3",
    "parcel-bundler": "^1.12.4",
    "serve": "^11.2.0"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
